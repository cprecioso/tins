    "yargs": "^10.1.1"
  },
  "devDependencies": {
    "@types/execa": "^0.8.1",
    "@types/find-up": "^2.1.1",
    "@types/fs-extra": "^5.0.0",
    "@types/got": "^7.1.6",
    "@types/lodash.intersection": "^4.4.3",
    "@types/node": "^9.3.0",
    "@types/read-pkg": "^3.0.0",
    "@types/yargs": "^10.0.1",
    "typescript": "^2.6.2"
